{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "linebreak-style": [
      0,
      "error",
      "windows"
    ],
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-shadow": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}